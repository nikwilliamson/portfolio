{"ast":null,"code":"import Position from \"./components/position.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Position\n  },\n\n  data() {\n    return {\n      positions: []\n    };\n  },\n\n  async created() {\n    this.positions = await this.getPositions();\n  },\n\n  methods: {\n    getPositions: async () => {\n      const query = `{\n            jobCollection(order: [startDate_DESC]) {\n              items {\n                sys {\n                  id\n                }\n                company\n                title\n                description2\n              }\n            }\n         }`;\n      const fetchUrl = `https://graphql.contentful.com/content/v1/spaces/${process.env.VUE_APP_CONTENTFUL_SPACE_ID}`;\n      const fetchOptions = {\n        method: \"POST\",\n        headers: {\n          Authorization: `Bearer ${process.env.VUE_APP_CONTENTFUL_ACCESS_TOKEN}`,\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          query\n        })\n      };\n\n      try {\n        const response = await fetch(fetchUrl, fetchOptions).then(response => response.json());\n        return response.data.jobCollection.items;\n      } catch (error) {\n        // throw new Error(\"Could not receive the data from Contentful!\");\n        console.log(error);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,QAAP,MAAqB,2BAArB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AAAEF;AAAF,GAFC;;AAGbG,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AADN,KAAP;AAGD,GAPY;;AAQb,QAAMC,OAAN,GAAgB;AACd,SAAKD,SAAL,GAAiB,MAAM,KAAKE,YAAL,EAAvB;AACD,GAVY;;AAWbC,SAAO,EAAE;AACPD,gBAAY,EAAE,YAAY;AACxB,YAAME,KAAI,GAAK;;;;;;;;;;;WAAf;AAaA,YAAMC,QAAO,GAAK,oDAAmDC,OAAO,CAACC,GAAR,CAAYC,2BAA4B,EAA7G;AACA,YAAMC,YAAW,GAAI;AACnBC,cAAM,EAAE,MADW;AAEnBC,eAAO,EAAE;AACPC,uBAAa,EAAG,UAASN,OAAO,CAACC,GAAR,CAAYM,+BAAgC,EAD9D;AAEP,0BAAgB;AAFT,SAFU;AAMnBC,YAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;AACnBZ;AADmB,SAAf;AANa,OAArB;;AAWA,UAAI;AACF,cAAMa,QAAO,GAAI,