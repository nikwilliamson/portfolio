{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\n// import HelloWorld from \"./components/HelloWorld.vue\";\nexport default {\n  name: \"App\",\n  components: {},\n\n  data() {\n    return {\n      positions: []\n    };\n  },\n\n  async created() {\n    this.positions = await this.getPositions();\n  },\n\n  methods: {\n    getPositions: async () => {\n      const query = `{\n            jobCollection {\n              items {\n                sys {\n                  id\n                }\n                company\n                title\n                startDate\n                endDate\n                description2\n              }\n            }\n         }`;\n      const fetchUrl = `https://graphql.contentful.com/content/v1/spaces/${process.env.VUE_APP_CONTENTFUL_SPACE_ID}`;\n      const fetchOptions = {\n        method: \"POST\",\n        headers: {\n          Authorization: `Bearer ${process.env.VUE_APP_CONTENTFUL_ACCESS_TOKEN}`,\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          query\n        })\n      };\n\n      try {\n        const response = await fetch(fetchUrl, fetchOptions).then(response => response.json());\n        return response.data.jobCollectionQuery.items;\n      } catch (error) {\n        throw new Error(\"Could not receive the data from Contentful!\");\n      }\n    },\n\n    debug(event) {\n      console.log(event.getPositions);\n    }\n\n  }\n};","map":{"version":3,"mappings":";AASA;AAEA,eAAe;AACbA,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE,EAFC;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AADN,KAAP;AAGD,GAPY;;AAQb,QAAMC,OAAN,GAAgB;AACd,SAAKD,SAAL,GAAiB,MAAM,KAAKE,YAAL,EAAvB;AAED,GAXY;;AAYbC,SAAO,EAAE;AACPD,gBAAY,EAAE,YAAY;AACxB,YAAME,KAAI,GAAK;;;;;;;;;;;;;WAAf;AAeA,YAAMC,QAAO,GAAK,oDAAmDC,OAAO,CAACC,GAAR,CAAYC,2BAA4B,EAA7G;AACA,YAAMC,YAAW,GAAI;AACnBC,cAAM,EAAE,MADW;AAEnBC,eAAO,EAAE;AACPC,uBAAa,EAAG,UAASN,OAAO,CAACC,GAAR,CAAYM,+BAAgC,EAD9D;AAEP,0BAAgB;AAFT,SAFU;AAMnBC,YAAI,EAAEC,IAAI,C